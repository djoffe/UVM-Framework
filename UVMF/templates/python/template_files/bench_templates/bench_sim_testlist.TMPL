{% set fname = "{{bench_location}}/{{name}}/sim/testlist" %}

{% block description %}

{% endblock %}

{% block contents %}

# Test list for use by RMDB file
# File syntax is
# TEST <testname> <repeat_count> <1st_seed> ... <nth seed>
# If not enough seeds are provided then random seeds are used to pad
# If no repeat count is given, default is 1
# Relative path to UVMF_VIP_LIBRARY_HOME and UVMF_PROJECT_DIR is from within the VRMDATA
#  structure, can be replaced with absolute path if desired
TB_INFO {{name}} { UVMF_VIP_LIBRARY_HOME=../../../../../{{relative_vip_from_sim}} UVMF_PROJECT_DIR=../../../../../../../{{name}} } { {% if svLibNames|length > 0 %}{% for svLibName in svLibNames %} -sv_lib ../../build_task/{{svLibName}}{% endfor %}{% endif %} }
TB {{name}}
# pragma uvmf custom regression_suite begin
TEST test_top 3
{% if inFactEnabled %}
TEST infact_test_base 3
{% endif %}
# pragma uvmf custom regression_suite end


{% endblock %}

